{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages/*":["pages/*"],
      "@icons/*":["public/icons/*"],
      "@lib/*":["src/lib/interface/index/*"]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
  },
  "include": [
    "next-env.d.ts",
    "pages/**/*",
    "src/**/*",
 "next.config.js" ],
  "exclude": [
    "node_modules"
  ]
}




